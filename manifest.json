{
  "name": "Hide fb news",
  "version": "1.0",
  "description": "Hide your fb news to save time.",
  "content_scripts": [{
      "matches": ["*://*.facebook.com/*"],
      "js": ["jquery.min.js", "kill.js"],
      "run_at": "document_start"
  }],
  "icons": {
    "16": "images/icon-red.png",
    "48": "images/icon-red.png",
    "128": "images/icon-red.png"
  },
  "browser_action": {
    "default_title": "Hide fb news",
    "default_icon": "images/icon.png"
  },
  "manifest_version": 2
}
